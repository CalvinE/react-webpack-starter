{
  "name": "react-webpack-starter",
  "version": "1.0.0",
  "description": "A repo to serve as a starting point for apps using react and webpack",
  "main": "index.js",
  "scripts": {
    "test": "mocha **/*.test.jsx --compilers js:babel-core/register --recursive",
    "dev": "NODE_ENV=DEV webpack-dev-server",
    "prod": "NODE_ENV=PROD webpack-dev-server",
    "build:dev": "NODE_ENV=dev webpack",
    "build:prod": "NODE_ENV=PROD webpack -p",
    "build:prodsm": "OVERRIDE_SOURCE_MAP=true NODE_ENV=prod webpack -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CalvinE/react-webpack-starter.git"
